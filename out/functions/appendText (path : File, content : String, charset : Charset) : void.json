{ "group" : "File_IO", "summary" : "This function appends a string of characters onto a file.", "signature" : "appendText (path : <a href='https://docs.oracle.com/javase/7/docs/api/java/io/File.html'>File</a>, content : <a href='https://docs.oracle.com/javase/7/docs/api/java/lang/String.html'>String</a>, charset : <a href='https://docs.oracle.com/javase/7/docs/api/java/nio/charset/Charset.html'>Charset</a>) : void", "formals" : [["path", "class java.io.File", "is the path to the file to read."], ["content", "class java.lang.String", "is the value to write to the file."], ["charset", "class java.nio.charset.Charset", "is the charset to use to encode the output. (may be null)"]], "returns" : ["void", "Nothing."], "throws" : [["<a href='https://docs.oracle.com/javase/7/docs/api/java/lang/NullPointerException.html'>NullPointerException</a>", "if <i>path</i> is null."], ["<a href='https://docs.oracle.com/javase/7/docs/api/java/lang/NullPointerException.html'>NullPointerException</a>", "if <i>content</i> is null."], ["<a href='https://docs.oracle.com/javase/7/docs/api/java/io/FileNotFoundException.html'>FileNotFoundException</a>", "if the file does not exist."], ["<a href='https://docs.oracle.com/javase/7/docs/api/java/io/IOException.html'>IOException</a>", "if the file cannot be written."]], "details" : [[0, "This overload assumes that the file is encoded using the default Charset."]] }