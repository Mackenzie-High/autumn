module Main in examples;

@Start
defun main (args : String[]) : void
{
    (My::print "Mercury", 1);
    (My::print "Venus", 2);
    (My::print "Earth", 3);
    (My::print "Mars", 4);

    (My::print "Jupiter", 5);
    (My::print "Saturn", 6);
    (My::print "Uranus", 7);
    (My::print "Neptune", 8);
}

defun print (planet : String, index : int) : void
{
    val type = (if index <= 4 then "Terrestrial" else "Jovian");

    (F::println planet .. " is " .. type);

    (F::println);
}