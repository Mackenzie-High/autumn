module Main in execution;

@Start
defun main (args : String[]) : void
{
    val primes = [2, 3, 5, 7, 11, 13, 17];

    val x = F::str(primes, "(", ", ", ")");
    val y = F::str(primes, "[", ", ", "]");
    val z = F::str(primes, "[", " | ", "]");

    F::println(x);
    F::println();

    F::println(y);
    F::println();

    F::println(z);
    F::println();
}