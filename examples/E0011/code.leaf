module Main in examples;

@Start
defun main (args : String[]) : void
{
    # Declare a new variable.
    var planet = "Venus";

    # Print the name of the planet.
    (F::println planet);

    # Change the value of the variable. 
    let planet = "Mars";

    # Print the name of the planet.
    (F::println planet);

    # Change the value of the variable. 
    planet = "Pluto";

    # Print the name of the planet.
    (F::println planet);
} 

