module * in execution;

@Start
defun main (args : String[]) : void
{
    assert (50B - 2B) == 48B;

    assert (50S - 2S) == 48S;

    assert (50  - 2)  == 48;

    assert (50L - 2L) == 48L;

    assert (50.0F - 2.0F) == 48.0F;

    assert (50.0 - 2.0) == 48.0;

    assert ((F::big 50) - (F::big 2)) == (F::big 48);

    assert (((F::big 50.0) - (F::big 2.0)).intValue) == 48;

    assert ((null is BigInteger) - (null is BigInteger)) == (F::big 0);

    assert ((null is BigDecimal) - (null is BigDecimal)) == (field BigDecimal::ZERO);
}





