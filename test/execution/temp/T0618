module Main in execution;

@Start
defun main (args : String[]) : void
{
    val original = new HashMap();

    original.put("A", 100);
    original.put("B", 200);
    original.put("C", 300);

    val copy = F::newMap(original is Map);

    original.put("X", 400);

    assert original.containsKey("A");
    assert original.containsKey("B");
    assert original.containsKey("C");
    assert original.containsKey("X");

    assert copy.containsKey("A");
    assert copy.containsKey("B");
    assert copy.containsKey("C");
    assert copy.containsKey("X") == false;
}