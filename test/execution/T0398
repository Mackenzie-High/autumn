module Main in execution;

functor Function (a : long) : void;

@Start
defun main (args : String[]) : void
{
    delegate p : Function => My::function;

    try
    {
        F::apply(p, ["Mars"]);

        assert false;
    }
    catch (ex : ClassCastException)
    {
        nop;
    }
}

defun function (a : long) : void
{
    assert false;
}