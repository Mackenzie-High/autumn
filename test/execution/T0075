module Main in execution;

@Start
defun main (args : String[]) : void
{
    var Z = false;
    var B = 0B;
    var S = 0S;
    var I = 0;
    var J = 0L;
    var F = 0.0F;
    var D = 0.0;
    var X = "";

    assert Z == false;
    assert B == 0B;
    assert S == 0S;
    assert I == 0;
    assert J == 0L;
    assert F == 0.0F;
    assert D == 0.0;
    assert X == "";

    let Z = true;
    let B = 13B;
    let S = 17S;
    let I = 21;
    let J = 23L;
    let F = 27.0F;
    let D = 29.0;
    let X = "Venus";

    assert Z == true;
    assert B == 13B;
    assert S == 17S;
    assert I == 21;
    assert J == 23L;
    assert F == 27.0F;
    assert D == 29.0;
    assert X == "Venus";
}