module Main in execution;

import execution.Helpers;

design Values
{
    data z : boolean;

    data c : char;

    data b : byte;

    data s : short;

    data i : int;

    data j : long;

    data f : float;

    data d : double;

    data o : String;
}

@Start
defun main (args : String[]) : void
{
    val p0 = (create Values);

    val p1 = (p0.z true);
    val p2 = (p1.c 'X');
    val p3 = (p2.b 10B);
    val p4 = (p3.s 20S);
    val p5 = (p4.i 30);
    val p6 = (p5.j 40L);
    val p7 = (p6.f 50.0F);
    val p8 = (p7.d 60.0);
    val p9 = (p8.o "Venus");

    assert (p9.z) == true;
    assert (p9.c) == 'X';
    assert (p9.b) == 10B;
    assert (p9.s) == 20S;
    assert (p9.i) == 30;
    assert (p9.j) == 40L;
    assert (p9.f) == 50.0F;
    assert (p9.d) == 60.0;
    assert (p9.o) == "Venus";

    assert (Helpers::unique [p0, p1, p2, p3, p4, p5, p6, p7, p8, p9]);
}