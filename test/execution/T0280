module Main in execution;


design Person (name : String, age : int);

design Employee () is Person;


@Start
defun main (args : String[]) : void
{
    val owner = (class Employee);

    assert owner != null;



    # Check whether the getter methods were generated.
    #
    assert ((Reflect::getMethod owner, "name", []).getReturnType) == (class String);
    assert ((Reflect::getMethod owner, "age", []).getReturnType) == (class int);



    # Check whether the setter methods were generated.
    #
    assert ((Reflect::getMethod owner, "name", [(class String)]).getReturnType) == owner;
    assert ((Reflect::getMethod owner, "age",  [(class int)]).getReturnType) == owner;
}
