module Main in execution;

tuple T (a : boolean,
         b : char,
         c : byte,
         d : short,
         e : int,
         f : long,
         g : float,
         h : double,
         i : String);

@Start
defun main (args : String[]) : void
{
    val t = (new T true, 'X', 10B, 20S, 30, 40L, 50.0F, 60.0, "Venus");

    val map = (t.toMap);

    assert (map.get "a") == true;
    assert (map.get "b") == 'X';
    assert (map.get "c") == 10B;
    assert (map.get "d") == 20S;
    assert (map.get "e") == 30;
    assert (map.get "f") == 40L;
    assert (map.get "g") == 50.0F;
    assert (map.get "h") == 60.0;
    assert (map.get "i") == "Venus";
}