module Main in execution;

tuple T (x : int);

@Start
defun main (args : String[]) : void
{
    var t = (new T 123);

    try
    {
        (t.set "y", 200);

        assert false;
    }
    catch(ex : NoSuchElementException)
    {
        // Pass, because this is what is supposed to happen.
    }
}