module Main in execution;

@Start
defun main (args : String[]) : void
{
    assert [1, 13] == (Main::G 13);
    assert [2, 17] == (Main::G 17);
    assert [3, 21] == (Main::G 21);
    assert [3, 23] == (Main::G 23);
}

defun G(x : int) : List
{
    yield  [1, x];
    yield  [2, x];
    return [3, x];
}