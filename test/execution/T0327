module Main in execution;

annotation Author;

@Start
@Author ("Mackenzie \t High")
defun main (args : String[]) : void
{
    val method = Reflect::getMethod((class Main), "main", [(class String[])]);

    assert method.isAnnotationPresent((class Author));

    val anno = method.getAnnotation((class Author));

    assert Reflect::getAnnotationValue(anno) == "Mackenzie " .. 9C .. " High";
}