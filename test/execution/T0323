module Main in program;

@Start
defun main (args : String[]) : void
{
    assert My::fib(46) == 1836311903L;

    assert My::fib(92) == 7540113804746346429L;
}

@Memoize
defun fib (n : long) : long
{
    when (n == 0) then return 0;

    when (n == 1) then return 1;

    return My::fib(n - 1) + My::fib(n - 2);
}