module Main in execution;

@Start
defun main (args : String[]) : void
{
    assert (50B / 2B) == 25B;

    assert (50S / 2S) == 25S;

    assert (50  / 2)  == 25;

    assert (50L / 2L) == 25L;

    assert (50.0F / 2.0F) == 25.0F;

    assert (50.0 / 2.0) == 25.0;

    assert 50BI / 2BI == 25BI;

    assert 50.0BD / 2.0BD == 25.0BD;

    assert ((null is BigInteger) / (null is BigInteger)) == 0BI;

    assert ((null is BigDecimal) / (null is BigDecimal)) == 0.0BD;
}