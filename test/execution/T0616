module Main in execution;

@Start
defun main (args : String[]) : void
{
    val original = new TreeSet(["A", "B", "C"]);

    val copy = F::newSet(original is Iterable);

    original.add("X");

    assert original == new TreeSet(["A", "B", "C", "X"]);

    assert copy == new TreeSet(["A", "B", "C"]);
}