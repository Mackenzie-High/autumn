module * in execution;

@Start
defun main (args : String[]) : void
{
    assert (50B * 2B) == 100B;

    assert (50S * 2S) == 100S;

    assert (50  * 2)  == 100;

    assert (50L * 2L) == 100L;

    assert (50.0F * 2.0F) == 100.0F;

    assert (50.0 * 2.0) == 100.0;

    assert 50BI * 2BI == 100BI;

    assert 50.0BD * 2.0BD == 100.0BD;

    assert ((null is BigInteger) * (null is BigInteger)) == 0BI;

    assert ((null is BigDecimal) * (null is BigDecimal)) == 0.0BD;
}