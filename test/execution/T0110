module Main in execution;

@Start
defun main (args : String[]) : void
{
    # Case: Empty
    {
    }

    # Case: One Statement
    {
        assert true;
    }

    # Case: Two Statements
    {
        assert true;
        assert true;
    }

    # Case: Three Statements
    {
        assert true;
        assert true;
        assert true;
    }
}