module Main in execution;

import execution.Helpers;

@Start
defun main (args : String[]) : void
{
    val functor = (call Helpers::newFibonacciFunctor);

    assert "1" == (call (funcall functor, 0).toString);
    assert "1" == (call (funcall functor, 1).toString);
    assert "2" == (call (funcall functor, 2).toString);
    assert "3" == (call (funcall functor, 3).toString);
    assert "5" == (call (funcall functor, 4).toString);
    assert "8" == (call (funcall functor, 5).toString);
}