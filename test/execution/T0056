module Main in execution;

@Start
defun main (args : String[]) : void
{
    # Integers
    assert 10C + 20C == 30C;
    assert 10B + 20B == 30B;
    assert 10S + 20S == 30S;
    assert 10  + 20  == 30;
    assert 10L + 20L == 30L;

    # Floats
    assert 10.0F + 20.0F > 29.0F;
    assert 10.0F + 20.0F < 31.0F;

    # Doubles
    assert 10.0 + 20.0 > 29.0;
    assert 10.0 + 20.0 < 31.0;
}