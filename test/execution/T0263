module Main in execution;

tuple Person (name : String);


@Start
defun main (args : String[]) : void
{
    delegate method : MethodHashCode => My::handler;

    val p = (new Person "Molly");

    val q = (Records::bindHash p, method);

    assert (p.hashCode) != (q.hashCode);
    assert (q.hashCode) == 17;
}

defun handler (self : Person) : int
{
    return 17;
}