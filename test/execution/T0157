module Main in execution;

design Person
{
    method tax (income : int) : int;
}

@Start
defun main (args : String[]) : void
{
    var p = (create Person);

    method p.tax => Main::moo;

    assert (p.tax 1000) == 250;
}

defun moo (self : Prototype, income : int) : int
{
    # 25% Tax Rate
    return income / 4;
}