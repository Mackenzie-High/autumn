module * in execution;

@Start
defun main (args : String[]) : void
{
    assert (55B % 4B) == 3B;

    assert (55S % 4S) == 3S;

    assert (55  % 4)  == 3;

    assert (55L % 4L) == 3L;

    assert (55.0F % 4.0F) == 3.0F;

    assert (55.0 % 4.0) == 3.0;

    assert 50BI % 4BI == 3BI;

    assert 50.0BD % 4.0BD == 3.0BD;

    assert ((null is BigInteger) % (null is BigInteger)) == 0BI;

    assert ((null is BigDecimal) % (null is BigDecimal)) == 0.0BD;
}