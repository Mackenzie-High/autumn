module Main in execution;

struct Person (name : String);

@Start
defun main (args : String[]) : void
{
    # The returned reference should not be null.
    assert (create Person) !== null;

    # The same object must be returned, because (create Person) simply reads a field.
    assert (create Person) === (create Person);
    assert (create Person) === (create Person);
    assert (create Person) === (create Person);
}