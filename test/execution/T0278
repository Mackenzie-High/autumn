module Main in execution;

@Start
defun main (args : String[]) : void
{
    assert (My::tester -2) == 23;
    assert (My::tester -1) == 23;
    assert (My::tester  0) == 100;
    assert (My::tester  1) == 23;
    assert (My::tester  2) == 23;
}

defun tester (x : int) : int
{
    branch (x) (CASE_0) default CASE_ELSE;

        assert false;

    marker CASE_0;

        return 100;

    marker CASE_ELSE;

        return 23;
}