module Main in execution;

@Start
defun main (args : String[]) : void
{
    assert (- - 3B) == 3B;

    assert (- - 3S) == 3S;

    assert (- - 3) == 3;

    assert (- - 3L) == 3L;

    assert (- - 3.0F) == 3.0F;

    assert (- - 3.0) == 3.0;

    assert (- - F::big(3)) == 3BI;

    assert (- - F::big(3.0)) == 3.0BD;

    assert (- - (null is BigInteger)) == 0BI;

    assert (- - (null is BigDecimal)) == 0.0BD;
}