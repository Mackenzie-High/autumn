module Main in tests;

@Start
defun main(args : String[]) : void
{
    var input = null is String;
    val output = null is JsonValue;

    input = "[12, -34.0, 56]";
    output = Json::parse(input);

    assert F::str(((output is JsonArray).get 0)) == "12";
    assert F::str(((output is JsonArray).get 1)) == "-34.0";
    assert F::str(((output is JsonArray).get 2)) == "56";
}