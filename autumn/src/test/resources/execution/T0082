module Main in execution;

@Start
defun main (args : String[]) : void
{
    # Note: The loop is used to detect operand-stack leakage.

    var i = 0;

    while(i < 25000)
    {
        var value_Z = true;
        var value_C = 77C;
        var value_B = 17B;
        var value_S = 17S;
        var value_I = 17;
        var value_J = 17L;
        var value_F = 17.0F;
        var value_D = 17.0;
        var value_T = "Soi";

        assert (value_Z is String) == "true";
        assert (value_C is String) == "M";
        assert (value_B is String) == "17";
        assert (value_S is String) == "17";
        assert (value_I is String) == "17";
        assert (value_J is String) == "17";

        assert ((value_F is int) as String) == "17";
        assert ((value_D is int) as String) == "17";

        assert (value_T is String) == "Soi";

        let i = i + 1;
    }
}