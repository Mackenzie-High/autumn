module Main in execution;

@Start
defun main (args : String[]) : void
{
    assert call Main::F01() == false;
    assert call Main::F02() == true;
    assert call Main::F03() == 'C';
    assert call Main::F04() == 101B;
    assert call Main::F05() == 103S;
    assert call Main::F06() == 107;
    assert call Main::F07() == 111L;
    assert call Main::F08() == 113.0F;
    assert call Main::F09() == 117.0;
    assert call Main::F10() == "Uranus";
    assert call Main::F11() == "Neptune";
}

defun F01() : boolean
{
    return false;
}

defun F02() : boolean
{
    return true;
}

defun F03() : char
{
    return 'C';
}

defun F04() : byte
{
    return 101B;
}

defun F05() : short
{
    return 103S;
}

defun F06() : int
{
    return 107;
}

defun F07() : long
{
    return 111L;
}

defun F08() : float
{
    return 113.0F;
}

defun F09() : double
{
    return 117.0;
}

defun F10() : Object
{
    return "Uranus";
}

defun F11() : String
{
    return "Neptune";
}



