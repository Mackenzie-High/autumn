module Main in execution;

@Start
defun main (args : String[]) : void
{
    assert F::defaultValueOf((class boolean)) == false;

    assert F::defaultValueOf((class char)) == 0C;

    assert F::defaultValueOf((class byte)) == 0B;

    assert F::defaultValueOf((class short)) == 0S;

    assert F::defaultValueOf((class int)) == 0;

    assert F::defaultValueOf((class long)) == 0L;

    assert F::defaultValueOf((class float)) == 0.0F;

    assert F::defaultValueOf((class double)) == 0.0;

    assert F::defaultValueOf((class BigInteger)) == null;

    assert F::defaultValueOf((class BigDecimal)) == null;

    assert F::defaultValueOf((class String)) == null;

    assert F::defaultValueOf((class Object)) == null;

    assert F::defaultValueOf((class List)) == null;
}